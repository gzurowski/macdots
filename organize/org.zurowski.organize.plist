<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>org.zurowski.organize</string>

    <key>ProgramArguments</key>
    <array>
        <string>/opt/homebrew/bin/organize</string>
        <string>run</string>
        <string>/Users/gregor/.config/organize</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>9</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StartInterval</key>
    <integer>86400</integer>

    <key>StandardOutPath</key>
    <string>/Users/gregor/Library/Logs/organize.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/gregor/Library/Logs/organize.err.log</string>

    <key>RunAtLoad</key>
    <true/>
</dict>
</plist>
