{
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "[graphql]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features",
    "editor.formatOnSave": false
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "[scminput]": {
    "editor.rulers": [
      50,
      72
    ],
    "editor.wordWrap": "wordWrapColumn"
  },
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.tabSize": 2
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "dart.previewFlutterUiGuides": true,
  "dart.previewFlutterUiGuidesCustomTracking": true,
  "debug.toolBarLocation": "commandCenter",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": "modificationsIfAvailable",
  "editor.fontFamily": "Jetbrains Mono, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 12.6,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "boundary",
  "git.blame.editorDecoration.enabled": true,
  "git.blame.statusBarItem.enabled": true,
  "git.mergeEditor": true,
  "github.copilot.editor.enableAutoCompletions": true,
  "terminal.integrated.fontFamily": "MesloLGSNFM-Regular",
  "terminal.integrated.copyOnSelection": true,
  "terraform.languageServer.terraform.path": "/usr/local/bin/terraform",
  "workbench.colorCustomizations": {
    "[Default Light+]": {
      "dart.closingLabels": "#99999959",
      "sideBar.foreground": "#000000"
    }
  },
  "workbench.colorTheme": "Default Light+",
  "workbench.iconTheme": "vscode-icons",
  "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "always"
}